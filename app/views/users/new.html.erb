<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>

<%= form_for @user do |f| %>
    Username:
    <%= f.text_field :name %><br>
    Password:
    <%= f.password_field :password %><br>
    Confirm password:
    <%= f.password_field :password_confirmation %><br>
    <%= f.submit :submit %>
<% end %>